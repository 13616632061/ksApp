<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@id/linearlayout01"
    android:background="@drawable/selector_cell_pressed"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="horizontal"
        android:padding="16dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginRight="20dp"
                android:visibility="gone"
                android:id="@id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color"
                android:textSize="14sp"
                android:text=""/>
            <TextView
                android:id="@id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color"
                android:textSize="14sp"
                android:text=""/>
        </LinearLayout>

        <TextView
            android:gravity="right"
            android:layout_weight="1"
            android:singleLine="false"
            android:textColor="@color/text_hint_color"
            android:id="@id/textView10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:text="ss"/>
    </LinearLayout>
    <TextView
        android:background="@color/divider_color"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/divider_height" />
    <LinearLayout
        android:padding="@dimen/listview_padding"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <!--<TextView-->
                <!--android:id="@id/textView4"-->
                <!--android:textSize="14sp"-->
                <!--android:textColor="@color/text_color"-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content" />-->
            <ImageView
                android:id="@id/imageView1"
                android:layout_width="20dp"
                android:layout_height="20dp" />
            <TextView
                android:text=""
                android:layout_marginLeft="3dp"
                android:id="@id/textView5"
                android:textSize="14sp"
                android:textColor="@color/text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:gravity="right"
                android:textSize="14sp"
                android:id="@id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""/>
        </LinearLayout>

        <LinearLayout
            android:gravity="center_vertical"
            android:visibility="gone"
            android:id="@id/linearLayout5"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/s_component_height">
            <TextView
                android:text=""
                android:gravity="center_vertical"
                android:id="@id/textView6"
                android:textSize="14sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color"/>
            <LinearLayout
                android:id="@id/linearLayout2"
                android:gravity="right"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="horizontal">
                <TextView
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:clickable="true"
                    android:id="@id/editText1"
                    android:background="@drawable/selector_flat_text"
                    android:textSize="14sp"
                    android:textColor="@color/accent_color"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/op_button_height"
                    android:text="点击确认"/>
                <TextView
                    android:gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="center_vertical"
                    android:clickable="true"
                    android:id="@id/editText2"
                    android:background="@drawable/selector_flat_text"
                    android:textSize="14sp"
                    android:textColor="@color/accent_color"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/op_button_height"
                    android:text="取消"/>
            </LinearLayout>
        </LinearLayout>

        <!--二维码部分开始-->
        <LinearLayout
            android:gravity="center_vertical"
            android:visibility="gone"
            android:id="@id/linearLayout6"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/s_component_height">
            <LinearLayout
                android:visibility="gone"
                android:id="@+id/qrcode_rl"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:src="@drawable/qrcode"
                    android:layout_width="20dp"
                    android:layout_height="20dp" />
                <TextView
                    android:text="付款码"
                    android:layout_marginLeft="3dp"
                    android:textSize="14sp"
                    android:textColor="@color/text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <TextView
                android:gravity="right"
                android:visibility="gone"
                android:textSize="14sp"
                android:id="@+id/desk_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""/>

        </LinearLayout>
        <!--二维码部分结束-->
    </LinearLayout>


    <include layout="@layout/set_line_grey_spacer" />
</LinearLayout>