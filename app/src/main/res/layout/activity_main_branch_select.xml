<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_branch_select"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:clipToPadding="false"
    android:background="?gray_bg"
    tools:context="com.ui.ks.MainBranchSelectActivity">
    <include
        layout="@layout/toolbar" android:id="@id/toolbar_layout" />
    <include
        layout="@layout/mainbranch_select"></include>
    <View
        android:layout_height="1dp"
        android:layout_width="match_parent"
        android:background="#ecf0f3"
        />

    <android.support.v4.view.ViewPager
        android:id="@+id/fragment_content"
        android:layout_height="match_parent"
        android:layout_width="match_parent"/>

</LinearLayout>
